<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="s6.frameop.object.Utilisateur" table="utilisateur">
        <id name="id" type="int" column="idutilisateur">
            <generator class="identity">
            </generator>
        </id>
        <property name="nom" column="nomutilisateur" type="string"/>
        <property name="email" column="email" type="string"/>
        <property name="password" column="password" type="string"/>
        <property name="privilege" column="privilege" type="int"/>
        <property name="statut" column="statut" type="int"/>
    </class>
</hibernate-mapping>